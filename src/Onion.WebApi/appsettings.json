{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Sql": "SQL-CONNECTION-STRING",
    "MongoDb": "MONGODB-CONNECTION-STRING"
  },
  "MongoDbSettings": {
    "Default": {
      "DatabaseName": "MONGO-DATABASE-NAME",
      "ConnectionString": "MONGO-CONNECTION-STRING"
    }
  },
  "AllowedHosts": "*",
  "TokenProviderSettings": {
    "SigningKey": "JWT-SIGNING-KEY"
  },
  "ApplicationSettings": {
    "AccessTokenLifetime": 5,
    "RefreshTokenLifetime":  10080
  },
  "GoogleAuthSettings": {
    "ClientId": "GOOGLE-CLIENT-ID",
    "ClientSecret": "GOOGLE-CLIENT-SECRET"
  }
}